task main()
{
  SetPower(OUT_A, 1);
}

sub shiftweight()
{
  int i = 0;
  i +-= -1;
  i /= 1;
  i += 2;
  i -= 1;
  i &= 1;
  i ||= 2;
  i |= 2;
  i++;

  i--;

  i = 0x1a;
  i = 10;

